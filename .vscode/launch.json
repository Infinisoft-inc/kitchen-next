  "version": "0.2.0",
  "configurations": [
    {
      "name": "Components Live Reload",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:8080",
      "webRoot": "${workspaceFolder}",
      "breakOnLoad": true,
      "sourceMaps": true,
      "sourceMapPathOverrides": {
        "webpack:/*": "/com.docker.devenvironments.code/packages/components/*"      }
    }
  ]
}
