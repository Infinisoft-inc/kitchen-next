{
  "name": "@infini-soft/lib-federation",
  "version": "0.0.11",
  "description": "Module federation helpers",
  "author": "Infinisoft Inc. <info@infini-soft.com>",
  "homepage": "https://www.kitchen.infini-soft.com/",
  "license": "INFINISOFT",
  "main": "dist/libfederation.js",
  "browser":"dist/libfederation.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build:dev": "yarn run build:types& webpack -c webpack.config.js",
    "build:dev:watch": "webpack -c webpack.config.js -w",
    "test": "jest",
    "test:coverage": "jest --coverage&& start coverage/lcov-report/index.html",
    "build:analyze": "webpack -c webpack.analyze.js",
    "build:analyze:baseline": "webpack -c webpack.analyze.js --env ANALYZEBASELINE=true",
    "build:types": "tsc -p tsconfig.json -d --emitDeclarationOnly --outDir dist --esModuleInterop",
    "build:types:watch": "tsc -p tsconfig.json -d --emitDeclarationOnly --outDir dist  --esModuleInterop -w",
    "build:prod": "yarn run build:types& webpack -c webpack.config.prod.js",
    "deploy": "kli component deploy",
    "start:dev": "webpack serve -c webpack.config.js --open",
    "serve:dist": "serve -s dist",
    "clean": "rm -rf analyze dist node_modules"
  },
  "devDependencies": {},
  "peerDependencies": {},
  "infinisoft": {
    "port": 8088
  }
}
