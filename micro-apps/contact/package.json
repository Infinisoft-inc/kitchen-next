{
  "name": "contact",
  "version": "0.0.68",
  "private": true,
  "description": "",
  "author": "Infinisoft Inc. <info@infini-soft.com>",
  "homepage": "https://www.kitchen.infini-soft.com/microapp/contacts",
  "license": "MIT",
  "scripts": {
    "build:types": "tsc -p tsconfig.json -d --emitDeclarationOnly --outFile dist/types.d.ts --esModuleInterop",
    "build:dev": "yarn run build:types& webpack -c webpack.config.dev.js",
    "build:dev:watch": "yarn run build:types& webpack -c webpack.config.dev.js -w",
    "build:prod": "yarn run build:types& webpack -c webpack.config.prod.js",
    "build:prod:watch": "yarn run build:types& webpack -c webpack.config.prod.js -w",
    "build:analyze": "webpack -c webpack.analyze.js& start analyze/bundle-stats.html",
    "build:analyze:baseline": "webpack -c webpack.analyze.js --env ANALYZEBASELINE=true& start analyze/bundle-stats.html",
    "test": "jest",
    "test:watchAll": "jest --watchAll",
    "test:coverage": "jest --coverage&& start analyze/coverage/lcov-report/index.html",
    "deploy": "kli component deploy",
    "start:dev": "yarn run webpack serve -c ./webpack.config.dev.js --open",
    "start:prod": "webpack serve -c ./webpack.config.prod.js --open",
    "serve:dist": "serve -s dist",
    "clean": "rm -rf analyze dist node_modules"
  },
  "devDependencies": {
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4"
  },
  "dependencies": {
    "@ant-design/pro-table": "^2.72.0",
    "@infini-soft/hooks-theme": "^0.0.14",
    "@infini-soft/lib-federation": "^0.0.14",
    "@infini-soft/promises": "^1.1.33",
    "@infini-soft/utils": "^1.3.37",
    "antd": "4.19.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-router-dom": "^6.3.0"
  },
  "infinisoft": {
    "component": "contact",
    "moduleFederation": {
      "remotes": {
        "flexline": "flexline@https://app.micro.infini-soft.com/flexline/remoteEntry.js",
        "flexcol": "flexcol@https://app.micro.infini-soft.com/flexcol/remoteEntry.js",
        "store": "store@https://app.micro.infini-soft.com/store/remoteEntry.js",
        "inputtext": "inputtext@https://app.micro.infini-soft.com/inputtext/remoteEntry.js",
        "crudlist": "crudlist@https://app.micro.infini-soft.com/crudlist/remoteEntry.js"
      }
    }
  }
}
